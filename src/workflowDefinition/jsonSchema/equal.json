{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "equal.json",

  "title": "Equal",
  "description": "Tests equality, with type coercion. Requires two arguments.",

  "type": "object",
  "additionalProperties": false,
  "required": ["=="],
  "properties": {
    "==": {
      "$ref": "binary-args.json"
    }
  },
  "examples": [{ "==": [1, 1] }, { "==": [1, "1"] }, { "==": [0, false] }]
}
