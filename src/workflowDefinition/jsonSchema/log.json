{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "log.json",

  "title": "log",
  "description": "Logs the first value to console, then passes it through unmodified.",
  "type": "object",
  "additionalProperties": false,
  "required": ["log"],
  "properties": {
    "log": {
      "$ref": "any.json"
    }
  },
  "examples": [{ "log": "apple" }]
}
