{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "strictEqual.json",

  "title": "Strict Equal",
  "description": "Tests strict equality. Requires two arguments.",

  "type": "object",
  "additionalProperties": false,
  "required": ["==="],
  "properties": {
    "===": {
      "$ref": "binary-args.json"
    }
  },
  "examples": [{ "===": [1, 1] }, { "===": [1, "1"] }]
}
