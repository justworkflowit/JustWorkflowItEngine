{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "subtract.json",

  "title": "-",
  "description": "Subtraction. Passing just one argument to - returns its arithmetic negative (additive inverse).",

  "type": "object",
  "additionalProperties": false,
  "required": ["-"],
  "properties": {
    "-": {
      "$ref": "binary-args.json"
    }
  },
  "examples": [{ "-": [4, 2] }, { "-": 2 }, { "-": -2 }]
}
